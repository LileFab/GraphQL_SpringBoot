# The Root Query for the application
type Query {
    getAllPlayers: [Player]
    getAllTeams: [Team]
}

# The Root Mutation for the application
type Mutation {
    addPlayer(id: String!, name: String!, age: Int!, height: Int!, team: String!) : Player!
    addTeam(id: String!, name: String!, city: String!, creationDate: String!): Team!
}